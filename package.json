  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.58",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.17.16",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
